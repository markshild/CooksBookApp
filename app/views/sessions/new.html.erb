<h2>Sign In</h2>

<%= render "layouts/errors" %>

<form action="<%= session_url %>" method="post">
  <%= auth_token_input %>

  <label>
    Username
  </label>
    <input type="text" name="user[email]" value="">

  <label>
    Password
  </label>
    <input type="password" name="user[password]" value="">

  <input type="submit" value="Sign In">
</form>
